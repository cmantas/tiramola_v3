     rm -rf /var/lib/ganglia/rrds/*
     rm /var/log/cassandra/system.log
     
     # find my IP
     my_priv_addr=""
     #look for an IPv4 interface in range 10.0 and use that for the cassandra communication
     let if_count=($(ifconfig | grep eth | wc -l ))-1
